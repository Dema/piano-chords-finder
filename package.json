{
  "name": "my-react-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"yarn dts:w\" vite",
    "build": "yarn dts && tsc && vite build",
    "serve": "vite preview",
    "dts": "tcm -e src -o src_gen",
    "dts:w": "tcm -e -w src -o src_gen",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "format:prettier": "prettier --write \"src/**/*.{ts,tsx}\" package.json",
    "format:eslint": "eslint --color --fix --cache \"src/**/*.{ts,tsx}\"",
    "format:stylelint": "stylelint --color --fix \"src/**/*.{ts,tsx}\"",
    "lint:unused-exports": "ts-unused-exports ./tsconfig.json --ignoreFiles=.stories.tsx | grep -v \\.generated",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --no-ignore --max-warnings=0 --fix",
      "stylelint --fix",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "*.{graphql,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@tonaljs/chord": "^4.5.1",
    "@tonaljs/chord-detect": "^4.5.1",
    "@tonaljs/midi": "^4.5.1",
    "@tonaljs/tonal": "^4.6.0",
    "abcjs": "6.0.0-beta.33",
    "classnames": "^2.2.6",
    "downshift": "^6.0.6",
    "midi-translate": "^1.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-piano": "^3.1.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "ts-essentials": "^7.0.1",
    "uuid": "^8.3.2",
    "webmidi": "^2.5.1"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.14",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "@types/vexflow": "^3.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "concurrently": "^6.1.0",
    "eslint": "^7.26.0",
    "eslint-config-adjunct": "4.11.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-array-func": "3.1.7",
    "eslint-plugin-deprecate": "0.7.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-github": "4.1.3",
    "eslint-plugin-html": "6.1.2",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jest-async": "1.0.3",
    "eslint-plugin-jest-dom": "3.9.0",
    "eslint-plugin-json": "3.0.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-local": "1.0.0",
    "eslint-plugin-markdown": "2.1.0",
    "eslint-plugin-no-constructor-bind": "2.0.4",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-no-unsanitized": "3.1.5",
    "eslint-plugin-no-use-extend-native": "0.5.0",
    "eslint-plugin-optimize-regex": "1.2.0",
    "eslint-plugin-postcss-modules": "1.2.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-scanjs-rules": "0.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sonarjs": "0.7.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "eslint-plugin-switch-case": "1.1.2",
    "eslint-plugin-unicorn": "32.0.1",
    "gh-pages": "^3.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-visualizer": "^5.5.0",
    "stylelint": "13.13.1",
    "stylelint-config-property-sort-order-smacss": "7.1.0",
    "stylelint-config-recommended": "5.0.0",
    "typed-css-modules": "^0.6.8",
    "typescript": "^4.1.2",
    "vite": "^2.3.3",
    "vite-plugin-compression": "^0.2.5"
  }
}
